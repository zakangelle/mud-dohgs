  "description": "A lightweight API and front end for viewing and managing the Mud Dohgs softball schedule.",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "start:prod": "cross-env NODE_ENV=production node server/index.js",
    "clean": "rimraf lib dist coverage",

    "react-modal": "^1.2.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-create-reducer": "^1.1.0",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^1.0.3"
